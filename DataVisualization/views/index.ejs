<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN">
<html lang="en">
	<head>
			<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
			<title>Test</title>

		<link rel='stylesheet' href='/stylesheets/navbar-fixed-side.css' />
		<link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
		<script src="//ajax.googleapis.com/ajax/libs/jquery/1/jquery.min.js" type="text/javascript" ></script>
		<script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js" type="text/javascript"></script>
		<script src="lib/prettyprint.js" type="text/javascript"></script>
		<script src="lib/jsonPath.js" type="text/javascript"></script>
		<script src="javascripts/apiHelper.js" type="text/javascript"></script>

	</head>
	<body>

	<form id="formPost" action="/draw" method="post">

	<div class="sidebar" style="float:left; margin-left:20px; margin-top:20px; width:20%">
		<ul class="nav navbar-fixed-side">

			<button type="button" class="btn btn-info" data-toggle="collapse" data-target="#col_getJSON">DATA SET</button>

			<div id="col_getJSON" class="collapse">

				<div class="input-group">
					<input id= "inputUrl" name="inputUrl" type="text" class="form-control" value="http://apis.skplanetx.com/melon/charts/realtime"
						   onClick="this.setSelectionRange(0, this.value.length)">
    				  <span class="input-group-btn">
       					 <button href="#myModal" class="btn btn-default" type="button" data-toggle="modal">
							 <span class="glyphicon glyphicon-cog"/>
						 </button>
     				 </span>
				</div>

				<button id ="btnREST" class="btn btn-default" type="button">GET!</button>
			</div>

				<div class="form-group" style="margin-top: 30px">
					<label>JSONPath</label>
					<input type="text" name="inputJSONPath" class="form-control" id="inputJSONPath" value="$..path">
				</div>

				<li><a href="#">Line</a></li>
				<li><a href="#">Bar</a></li>
				<li><a href="#">Chart</a></li>
				<li><a id="aRealtime">Realtime</a></li>

			   <input id="inputPost" type="button" class="btn btn-info" value="Draw">
		</ul>
	</div>
	</form>

	<div id="jsonChart" style="float:left; width:70%; margin-left:30px">
	</div>


	<div class="modal fade" id="myModal">
		<div class="modal-dialog">
			<div class="modal-content">
				<div class="modal-header">
					<button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
					<h3 class="modal-title">Setting Header</h3>
				</div>
				<div class="modal-body">
					<table class="table table-striped" id="tblGrid">
						<thead id="tblHead">
						<tr>
							<th>key</th>
							<th>value</th>
						</tr>
						</thead>
						<tbody id="tbodyTable">

						</tbody>
						<td>
							<input id="inputKey" type="text" placeholder='Key' class="form-control" onClick="this.setSelectionRange(0, this.value.length)"/>
						</td>
						<td>
							<input id="inputValue" type="text" placeholder='Value' class="form-control" onClick="this.setSelectionRange(0, this.value.length)"/>
						</td>
					</table>

					<div class="form-group">

						<input id="btnMinus" type="button" class="btn btn-danger pull-right" value="-" style="margin-left: 3px;">

						<input id="btnAdd" type="button" class="btn btn-warning pull-right" value="+">

						<div class="clearfix"></div>
					</div>
				</div>
				<div class="modal-footer">
					<button type="button" class="btn btn-default " data-dismiss="modal">Close</button>
					<button id="btnSave" type="button" class="btn btn-primary" data-dismiss="modal" >Save</button>
				</div>

			</div>
		</div>
	</div>
	</body>

	<script src="javascripts/index.js" type="text/javascript"></script>
	<script src="javascripts/struct.js" type="text/javascript"></script>

</html>
















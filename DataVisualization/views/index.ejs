<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/html">
<head>
    <title>Test</title>

    <% include drawer.ejs %>
    <% include indexHeader.html %>
</head>

<body>

<!--setting Toggle-->
<form id="formPost" action="/draw" method="post">

    <div id="sidebar" class="sidebar" style="float:left; margin-left:20px; margin-top:20px; width:20%">
        <ul id="navbar" class="nav navbar-fixed-side">


            <div id="col_getJSON">
                <!--192.168.0.203-->
                <!--value="http://support.jennifersoft.com:8000/api/realtime/domain"-->
                <div class="input-group">
                    <input id="inputUrl" name="inputUrl" type="text" class="form-control" value="192.168.0.203"
                           onClick="this.setSelectionRange(0, this.value.length)">
    				  <span class="input-group-btn">
       					 <button id="btnREST" class="btn btn-default" data-toggle="drawer" href="#drawerStruct"
                                 type="button">
                             <span class="glyphicon glyphicon-import"/>
                         </button>
     				 </span>
                </div>
            </div>

            <div class="form-group" style="margin-top: 5px">

                <label style="margin-top: 5px">JSONPath</label>

                <div id="btnJSON" class="btn btn-default pull-right">
                    <span class="glyphicon glyphicon-plus"/>
                </div>

                <div id="groupJSON" style="margin-top: 6px">
                    <input type="text" name="inputJSONPath" class="form-control" value="$..path">
                </div>

            </div>

            <button id="btnRegister" href="#modalHeader" class="btn btn-info" type="button" data-toggle="modal">
                DATA REGISTER
            </button>

            <!--value=-->

            <li style="margin-top: 30px">
                <a id="aChart" data-toggle="modal" href="#modalChart">Draw Realtime</a>
            </li>
        </ul>

    </div>
</form>


<!--chartClass-->

<div id="divChart"></div>

<!--setting Header modal-->

<div class="modal fade" id="modalHeader">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                <h3 class="modal-title">Setting MetaData</h3>
            </div>
            <div class="modal-body">

                <ul id="ulTabs" class="nav nav-tabs">
                    <li class="active"><a href="#divURL" data-toggle="tab">URL</a></li>
                    <li><a href="#divDB" data-toggle="tab">MySQL</a></li>
                </ul>

                <div class="tab-content clearfix">

                    <div class="tab-pane active" id="divURL">

                        <div align="center" style="font-weight:bold">Data Title</div>

                        <input id="inputTitle" type="text" placeholder='String' class="form-control"
                               onClick="this.setSelectionRange(0, this.value.length)"/>

                        <table class="table table-striped" id="tblGrid">
                            <thead id="tblHead">
                            <tr>
                                <th>Interval</th>
                                <th>Exist TimeColumn</th>
                            </tr>
                            </thead>

                            <tbody>

                            <td>
                                <input id="inputInterval" type="text" placeholder='Number' class="form-control"
                                       onClick="this.setSelectionRange(0, this.value.length)"/>
                            </td>
                            <td>
                                <div align="center">
                                    <input id="inputTimeCheck" type="checkbox" style="width:30px; height:30px;"/>
                                </div>
                            </td>
                            </tbody>

                            <thead id="tblHead">
                            <tr>
                                <th>key</th>
                                <th>value</th>
                            </tr>
                            </thead>

                            <tbody id="tbodyTable">

                            </tbody>
                            <thead id="tblFoot">
                            </thead>
                        </table>

                        <div class="form-group">

                            <input id="btnMinus" type="button" class="btn btn-danger pull-right" value="-"
                                   style="margin-left: 3px;">

                            <input id="btnAdd" type="button" class="btn btn-warning pull-right" value="+">

                            <div class="clearfix"></div>
                        </div>
                    </div>

                    <div class="tab-pane" id="divDB">

                        <div align="center" style="font-weight:bold">Data Title</div>


                        <input id="inputDBTitle" type="text" placeholder='String' class="form-control"
                               onClick="this.setSelectionRange(0, this.value.length)"/>

                        <table class="table table-striped" id="tblGrid">
                            <thead id="tblHead">
                            <tr>
                                <th colspan="2">Interval</th>
                                <th colspan="2">Exist TimeColumn</th>
                            </tr>
                            </thead>

                            <tbody>

                            <td colspan="2">
                                <input id="inputDBInterval" type="text" placeholder='Number' class="form-control" value="1000"
                                       onClick="this.setSelectionRange(0, this.value.length)"/>
                            </td>
                            <td colspan="2">
                                <div align="center">
                                    <input id="inputDBTimeCheck" type="checkbox" style="width:30px; height:30px;"/>
                                </div>
                            </td>
                            </tbody>

                            <thead id="tblHead">
                            <tr>
                                <th>connectionId</th>
                                <th>password</th>
                                <th>port</th>
                                <th>targetDB</th>
                            </tr>
                            </thead>

                            <tbody>

                            <td>
                                <input id="inputUser" type="text" placeholder='id' class="form-control" value="newuser"
                                       onClick="this.setSelectionRange(0, this.value.length)"/>
                            </td>
                            <td>
                                <input id="inputPassword" type="password" placeholder='password' class="form-control"
                                       value="asdf"
                                       onClick="this.setSelectionRange(0, this.value.length)"/>
                            </td>
                            <td>
                                <input id="inputPort" type="number" placeholder='port' class="form-control" value="3306"
                                       onClick="this.setSelectionRange(0, this.value.length)"/>
                            </td>
                            <td>
                                <input id="inputDatabase" type="text" placeholder='name' class="form-control"
                                       value="jennifer"
                                       onClick="this.setSelectionRange(0, this.value.length)"/>
                            </td>
                            </tbody>

                            <thead>
                            <td colspan="3">
                                <input id="inputTable" type="text" placeholder='Table' class="form-control"
                                       value="request"
                                       onClick="this.setSelectionRange(0, this.value.length)"/>
                            </td>
                            <td id="btnConnectDB" colspan="1" align="center">
                                <button class="btn-primary">connection</button>
                            </td>
                            </thead>

                            <thead id="tblHead">
                            <tr>
                                <th colspan="2">key</th>
                                <th colspan="2">value</th>
                            </tr>
                            </thead>

                            <tbody id="tbodyTable">

                            </tbody>

                            <thead id="dbTblfoot">

                            </thead>
                        </table>

                        <div class="form-group">

                            <input id="btnDBminus" type="button" class="btn btn-danger pull-right" value="-"
                                   style="margin-left: 3px;">

                            <input id="btnDBplus" type="button" class="btn btn-warning pull-right" value="+">

                            <div class="clearfix"></div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default " data-dismiss="modal">Close</button>
                    <button id="btnSave" type="button" class="btn btn-primary" data-dismiss="modal">Save</button>
                </div>

            </div>
        </div>
    </div>
</div>

<!--setting Chart modal-->

<div class="modal fade" id="modalChart">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                <h3 class="modal-title">Setting Chart</h3>
            </div>

            <div class="modal-body">

                <select id="selectTitle" class="form-control">
                </select>

                <div id="modalColumn">

                </div>

                <table class="table table-striped" id="tblGrid">
                    <thead id="tblHead">
                    <tr>
                        <th>Interval</th>
                        <th>Brush Type</th>
                    </tr>
                    </thead>

                    <tbody>
                    <td>
                        <input id="inputChartInterval" type="text" placeholder='Number' class="form-control"
                               onClick="this.setSelectionRange(0, this.value.length)"/>
                    </td>
                    <td>
                        <select id="selectChartType" class="form-control">
                            <option>line</option>
                            <option>scatter</option>
                        </select>
                    </td>
                    </tbody>
                </table>

                <table class="table table-striped">
                    <thead id="tblHead">
                    <tr>
                        <th>Time</th>
                        <th>How long</th>
                    </tr>
                    </thead>

                    <tbody>
                    <td>
                        <select id="selectTime" class="form-control">

                        </select>
                    </td>
                    <td>
                        <select id="selectChartTime" class="form-control">
                            <option>5minute</option>
                            <option>1hour</option>
                            <option>24hour</option>
                        </select>
                    </td>
                    </tbody>
                </table>


                <table class="table table-striped">
                    <thead>
                    <tr>
                        <th>color</th>
                        <th>key</th>
                        <th>value</th>
                    </tr>
                    </thead>
                    <tbody id="tbodyChartSet">

                    <tr id="trRow">
                        <th style="width : 40%">
                            <div id="divColorpicker" name="divColorpicker" class="input-group colorpicker-component">
                                <input type="text" name="inputColor" value="#00AABB" class="form-control"/>
                                <span class="input-group-addon"><i></i></span>
                            </div>
                        </th>
                        <th style="width : 30%">
                            <input id="inputKey" name="inputKey" class="form-control">
                            </input>
                        </th>
                        <th style="width : 30%">
                            <select id="selectValue" name="selectValue" class="form-control">
                            </select>
                        </th>
                    </tr>
                    </tbody>
                </table>


                <div class="form-group">

                    <input id="btnChartMinus" type="button" class="btn btn-danger pull-right" value="-"
                           style="margin-left: 3px;">

                    <input id="btnChartAdd" type="button" class="btn btn-warning pull-right" value="+">

                    <div class="clearfix"></div>
                </div>

            </div>

            <div class="modal-footer">
                <button type="button" class="btn btn-default " data-dismiss="modal">Close</button>
                <button id="btnSaveChart" type="button" class="btn btn-primary" data-dismiss="modal">Save</button>
            </div>

        </div>
    </div>
</div>


</body>

<script src="javascripts/index.js" type="text/javascript"></script>
<script src="javascripts/chartClass/dateChart.js" type="text/javascript"></script>
<script src="javascripts/chartClass/dayChart.js" type="text/javascript"></script>
<script src="javascripts/chartHelper.js" type="text/javascript"></script>

</html>















